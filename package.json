{
  "name": "MineChat",
  "private": true,
  "author": "yyp",
  "version": "0.0.1",
  "type": "commonjs",
  "main": "./electron/main.js",
  "description": "MineChat",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "build": "vue-tsc -b && vite build",
    "vite:build": "vite build",
    "electron:build": "vite build --config vite.config.electron.ts && electron-builder",
    "package": "electron-forge package",
    "electron:pack": "vue-tsc -b && vite build --config vite.config.electron.ts && electron-packager . MineChat --win --out ../MineChatApp --arch=x64 --app-version=0.0.1 --electron-version=32.1.2 --icon=./electron/icon/favicon.ico",
    "electron:package": "electron-packager .  MineChat --platform=win32 --out ../MineChatApp --arch=x64 --icon=./electron/icon/favicon.ic --ignore=node_modules/electron-*",
    "electron:package:win": "electron-packager . MineChat --win --out ../MineChatApp --arch=x64 --app-version=0.0.1 --electron-version=32.1.2 --icon=./electron/icon/favicon.ico",
    "electron:dev": "chcp 65001 && vite --config vite.config.electron.ts",
    "electron:test": "chcp 65001 && vite --port 3100 --config vite.config.electron.ts",
    "dev": "chcp 65001 && vite",
    "preview": "vite preview",
    "start": "concurrently \"vite --port 3000\" \"cross-env ELECTRON_PORT=3000 electron .\"",
    "test": "concurrently \"vite --port 3100\" \"cross-env ELECTRON_PORT=3100 electron .\""
  },
  "dependencies": {
    "@bytemd/vue-next": "^1.21.0",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@element-plus/icons-vue": "^2.3.1",
    "@icon-park/svg": "^1.4.2",
    "@icon-park/vue-next": "^1.4.2",
    "@imndx/v-emoji-picker-vue3": "^3.0.2",
    "@soerenmartius/vue3-clipboard": "^0.1.2",
    "@vueuse/components": "^10.8.0",
    "@vueuse/core": "^10.8.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "artplayer": "^5.2.1",
    "axios": "^1.7.7",
    "electron-screenshot-app": "^4.0.3",
    "electron-screenshots": "^0.5.27",
    "electron-vite": "^2.3.0",
    "element-plus": "^2.8.3",
    "moment": "^2.30.1",
    "nodemon": "^3.1.7",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "sqlite3": "^5.1.7",
    "tmp": "^0.2.3",
    "vue": "^3.4.37",
    "vue-image-lightbox": "^7.2.0",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.4.5",
    "vue-web-screen-shot": "^1.5.3",
    "vue3-emoji-picker": "^1.1.8",
    "vuex": "^4.0.2",
    "wangeditor": "^4.7.15",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@electron-forge/cli": "^7.6.0",
    "@types/node": "^22.5.5",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/cli-plugin-babel": "^5.0.8",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^32.1.2",
    "electron-builder": "^25.1.7",
    "electron-packager": "^17.1.2",
    "less": "^4.2.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-electron": "^0.29.0",
    "vue-tsc": "^2.0.29",
    "webpack": "^5.94.0"
  }
}
